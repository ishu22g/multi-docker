{
  "AWSEBDockerrunVersion": 2,
  "containerDefinitions": [
    {
      "name": "client",
      "image": "ishu22g/multi-client",
      "hostname": "client",
      "essential": false
    },
    {
      "name": "server",
      "image": "ishu22g/multi-server",
      "hostname": "api",
      "essential": false
    },
    {
      "name": "worker",
      "image": "ishu22g/multi-worker",
      "essential": false
    },
    {
      "name": "ngnix",
      "image": "ishu22g/multi-ngnix",
      "essential": true,
      "portMappings": [
        {
          "hostPort": 80,
          "containerPort": 80
        }
      ],
      "links": [
        "server",
        "client"
      ]
    }
  ]
}